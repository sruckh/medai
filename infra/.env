# Pick ONE provider by setting PROVIDER
# PROVIDER can be: HF_TGI or RUNPOD_OPENAI
PROVIDER=RUNPOD_OPENAI

# For Hugging Face Inference Endpoint (TGI Messages API)
HF_API_BASE=https://your-endpoint.endpoints.huggingface.cloud
HF_API_KEY=hf_xxx

# For RunPod vLLM OpenAI-compatible
RUNPOD_API_BASE=https://api.runpod.ai/v2/<ENDPOINT_ID>/runsync
RUNPOD_API_KEY=rp_xxx

# Model name to pass to provider
MODEL_NAME=Intelligent-Internet/II-Search-4B

# CORS
ALLOWED_ORIGINS=*
